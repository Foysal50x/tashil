{
    "name": "foysal50x/tahsil",
    "description": "A comprehensive Subscription Management System for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Foysal Ahmed",
            "email": "foysal@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/redis": "^10.0|^11.0|^12.0",
        "tpetry/laravel-query-expressions": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^9.0|^10.0",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Foysal50x\\Tashil\\": "src/",
            "Foysal50x\\Tashil\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foysal50x\\Tashil\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foysal50x\\Tashil\\Providers\\TashilServiceProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest",
        "test:unit": "pest --testsuite=Unit",
        "test:feature": "pest --testsuite=Feature"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}